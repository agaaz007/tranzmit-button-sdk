{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm build:core && pnpm build:embed && pnpm build:react",
  "functions": {
    "api/index.ts": {
      "includeFiles": "packages/embed/dist/**,packages/backend/src/**"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" }
  ]
}
