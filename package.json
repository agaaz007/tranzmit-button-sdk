{
  "name": "@tranzmit/exit-button-sdk",
  "version": "0.1.0",
  "private": true,
  "description": "Exit Button WebSDK - AI-powered churn prevention",
  "author": "TranzmitAI",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @tranzmit/exit-button-core build",
    "build:embed": "pnpm --filter @tranzmit/exit-button-embed build",
    "build:react": "pnpm --filter @tranzmit/exit-button-react build",
    "dev": "pnpm -r --parallel dev",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "@types/node": "^20.10.6",
    "vitest": "^3.1.0",
    "@vitest/coverage-v8": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.14.0"
}
